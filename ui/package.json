{
  "name": "towermod",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "build-rust": "napi build --platform --manifest-path ../rust/towermod/Cargo.toml --js index.cjs -o ./src/towermod",
    "start": "electron-vite preview",
    "dev": "npm run build-rust && electron-vite dev -w",
    "build": "npm run build-rust && npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "napi": {
    "binaryName": "towermod"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@electron/remote": "^2.1.2",
    "@karmaniverous/serify-deserify": "^2.0.11",
    "@reduxjs/toolkit": "^2.3.0",
    "electron-redux": "^2.0.0",
    "electron-updater": "^6.1.7",
    "glob": "^11.0.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-redux": "^9.1.2",
    "react-toastify": "^10.0.6",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@napi-rs/cli": "^3.0.0-alpha.63",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.80.3",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-plugin-static-copy": "^2.0.0",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
